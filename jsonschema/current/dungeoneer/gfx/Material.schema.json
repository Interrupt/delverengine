{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "title": "Material",
    "type": "object",
    "properties": {
        "class": {
            "const": "com.interrupt.dungeoneer.gfx.Material",
            "description": "Fully-qualified Java class name."
        },
        "texAtlas": {
            "type": "string",
            "description": "TextureAtlas name.",
